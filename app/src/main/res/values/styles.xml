<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="big_img_style">
        <item name="android:layout_width">@dimen/dp50</item>
        <item name="android:layout_height">@dimen/dp50</item>
        <item name="android:padding">@dimen/dp5</item>
    </style>

    <style name="normal_img_style">
        <item name="android:layout_width">@dimen/dp40</item>
        <item name="android:layout_height">@dimen/dp40</item>
        <item name="android:padding">@dimen/dp5</item>
    </style>

    <style name="small_img_style">
        <item name="android:layout_width">@dimen/dp35</item>
        <item name="android:layout_height">@dimen/dp35</item>
        <item name="android:padding">@dimen/dp5</item>
    </style>


    <style name="title_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="goods_title_style" parent="title_style">
        <item name="android:textColor">@color/textmiddle</item>
        <item name="android:textSize">@dimen/sp18</item>
        <item name="android:padding">@dimen/dp10</item>
    </style>

    <style name="wrap_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="radio_button_style" parent="wrap_text_style">
        <item name="android:textSize">@dimen/sp14</item>
        <item name="android:textColor">@color/bgwhite</item>
        <item name="android:background">@drawable/radio_button_bg</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="normal_white_text_style" parent="wrap_text_style">
        <item name="android:textColor">@color/bgwhite</item>

    </style>

    <style name="user_linear_item_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_margin">@dimen/dp5</item>
    </style>

    <style name="user_gray_text_style" parent="wrap_text_style">
        <item name="android:textSize">@dimen/sp_12</item>
        <item name="android:textColor">@color/textmiddle</item>
        <item name="android:text">@string/test</item>

    </style>

    <style name="small_img_no_padding_style">
        <item name="android:layout_width">@dimen/dp25</item>
        <item name="android:layout_height">@dimen/dp25</item>
    </style>

    <style name="mall_title_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">@dimen/dp10</item>
        <item name="android:textSize">@dimen/sp16</item>
        <item name="android:textColor">@color/textmiddle</item>

    </style>

    <style name="login_edit_style">

        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:textColorHint">@color/xlightGray</item>
        <item name="android:textSize">@dimen/sp16</item>

    </style>

    <style name="login_btn">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp40</item>
        <item name="android:textSize">@dimen/sp18</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_margin">@dimen/dp10</item>
        <item name="android:background">@drawable/login_sure_bg</item>
        <item name="android:gravity">center</item>

    </style>

    <style name="feedback_title_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/sp16</item>
        <item name="android:textColor">@color/textdark</item>
        <item name="android:padding">@dimen/dp5</item>

    </style>

    <style name="with_text_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/sp14</item>
        <item name="android:layout_margin">@dimen/dp5</item>
        <item name="android:gravity">center</item>
    </style>


    <style name="with_text_style_title" parent="with_text_style">

        <item name="android:textColor">@color/textdark</item>
        <!--<item name="android:textStyle">bold</item>-->
        <item name="android:textSize">@dimen/sp15</item>
    </style>

    <style name="with_edit_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/gray_input_bg</item>
        <item name="android:layout_margin">@dimen/dp5</item>
        <item name="android:padding">@dimen/dp5</item>
    </style>

    <style name="wallet_top_item">

        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">@dimen/dp20</item>
        <item name="android:paddingBottom">@dimen/dp20</item>

    </style>

    <style name="wallet_btn_style">
        <item name="android:layout_width">@dimen/dp80</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/orange_radio10_bg</item>
        <item name="android:textSize">@dimen/sp14</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="wallet_top_text_style">

        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">@dimen/dp5</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">@dimen/sp14</item>
        <item name="android:textColor">@color/textmiddle</item>


    </style>

    <style name="wallet_text_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@drawable/wallet_text_bg</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">@dimen/dp10</item>
    </style>

    <style name="interact_content_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/sp15</item>
        <item name="android:textColor">@color/textdark</item>
        <item name="android:layout_marginTop">@dimen/dp5</item>
        <item name="android:layout_marginBottom">@dimen/dp5</item>
        <item name="android:minLines">1</item>
    </style>

    <style name="connection_title_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/sp16</item>
        <item name="android:textColor">@color/textdark</item>
        <item name="android:padding">@dimen/dp5</item>
    </style>

    <style name="connection_text_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/sp15</item>
        <item name="android:textColor">@color/textmiddle</item>
        <item name="android:padding">@dimen/dp5</item>

    </style>

    <dimen name="dp30">30dp</dimen>
    <dimen name="dp25">25dp</dimen>
    <color name="white">#fff</color>
</resources>
